{
  "apiKey": "${API_KEY}",
  "origins": [
    {
      "http": {
        "url": "https://yourappname.herokuapp.com/graphql",
        "overrideRequestHeaders": {
          "Host": "yourappname.herokuapp.com"
        }
      }
    }
  ],
  "frontends": [
    {
      "host": "0.0.0.0",
      "port": ${PORT},
      "endpoint": "/graphql"
    }
  ],
  "stores": [
    {
      "name": "embeddedCache",
      "inMemory": {
        "cacheSize": 10485760
      }
    }
  ],
  "sessionAuth": {
    "store": "embeddedCache",
    "header": "Authorization"
  },
  "queryCache": {
    "publicFullQueryStore": "embeddedCache",
    "privateFullQueryStore": "embeddedCache"
  },
  "logging": {
    "level": "DEBUG"
  }
}
